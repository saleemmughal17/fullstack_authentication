
datasource db {
  provider = "postgresql"  // or "mysql" if using MySQL
  url      = env("DATABASE_URL")
}


generator client {
  provider = "prisma-client-js"
}

model User {
  id       String  @id @default(uuid())
  email    String  @unique
  password String
  role     String    

  @@map("users")
}

enum Role {
  ADMIN
  USER
}

